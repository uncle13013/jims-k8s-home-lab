# -*- mode: ruby -*-
# vi: set ft=ruby :

Vagrant.configure("2") do |config|
  config.vm.box = "ubuntu/jammy64"
  
  config.vm.define "test-ubuntu" do |ubuntu|
    ubuntu.vm.hostname = "test-ubuntu"
    ubuntu.vm.network "private_network", ip: "192.168.56.100"
    
    ubuntu.vm.provider "virtualbox" do |vb|
      vb.name = "test-ubuntu"
      vb.memory = "2048"
      vb.cpus = 1
      vb.gui = false
    end
  end
end
